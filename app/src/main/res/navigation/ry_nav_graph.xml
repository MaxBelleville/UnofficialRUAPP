<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ry_nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.letgo.ruapp.Home.HomeFragment"
        android:label="content_home"
        tools:layout="@layout/content_home" >
        <action
            android:id="@+id/action_homeFragment_to_assigmentsFragment"
            app:destination="@id/assigmentsCourses" />
        <action
            android:id="@+id/action_homeFragment_to_travelActivity"
            app:destination="@id/travelActivity" />
        <action
            android:id="@+id/action_homeFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
        <action
            android:id="@+id/action_homeFragment_to_assigmentsList"
            app:destination="@id/assigmentsList" />
        <action
            android:id="@+id/action_homeFragment_to_assigmentBlank"
            app:destination="@id/assigmentBlank" />
    </fragment>
    <fragment
        android:id="@+id/assigmentsCourses"
        android:name="com.letgo.ruapp.Assigments.AssigmentsCoursesFragment"
        android:label="content_assigments_courses"
        tools:layout="@layout/content_assignments_courses" >
        <action
            android:id="@+id/action_assigmentsCourses_to_webActivity"
            app:destination="@id/webActivity" />
        <action
            android:id="@+id/action_assigmentsCourses_to_assigmentsList"
            app:destination="@id/assigmentsList" />
    </fragment>
    <fragment
        android:id="@+id/assigmentsList"
        android:name="com.letgo.ruapp.Assigments.AssigmentsList"
        android:label="content_assigments_list"
        tools:layout="@layout/content_assignments_list" >
        <action
            android:id="@+id/action_assigmentsList_to_assigmentFragment"
            app:destination="@id/assigmentFragment" />
    </fragment>
    <fragment
        android:id="@+id/assigmentFragment"
        android:name="com.letgo.ruapp.Assigments.AssigmentFragment"
        android:label="content_assigment"
        tools:layout="@layout/content_assignment" >
        <action
            android:id="@+id/action_assigmentFragment_to_assignmentCreate"
            app:destination="@id/assignmentCreate" />
    </fragment>
    <fragment
        android:id="@+id/assigmentBlank"
        android:name="com.letgo.ruapp.Assigments.AssigmentBlank"
        android:label="content_assigment_blank"
        tools:layout="@layout/content_assignment_blank" >
        <action
            android:id="@+id/action_assigmentBlank_to_assignmentCreate"
            app:destination="@id/assignmentCreate" />
    </fragment>
    <activity
        android:id="@+id/webActivity"
        android:name="com.letgo.ruapp.WebActivity"
        android:label="activity_web"
        tools:layout="@layout/activity_web" />
    <activity
        android:id="@+id/travelActivity"
        android:name="com.letgo.ruapp.TravelActivity"
        android:label="activity_travel"
        tools:layout="@layout/activity_travel" />
    <fragment
        android:id="@+id/assignmentCreate"
        android:name="com.letgo.ruapp.Assigments.AssignmentCreate"
        android:label="content_assignment_create"
        tools:layout="@layout/content_assignment_create" />
    <fragment
        android:id="@+id/scheduleFragment"
        android:name="com.letgo.ruapp.Schedule.ScheduleFragment"
        android:label="content_schedule"
        tools:layout="@layout/content_schedule" >
        <action
            android:id="@+id/action_scheduleFragment_to_assigmentsList"
            app:destination="@id/assigmentsList" />
    </fragment>
</navigation>